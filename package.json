{
  "name": "@smartlyio/oats-fast-check",
  "version": "0.0.6",
  "license": "MIT",
  "description": "fast-check value generator for smartlyio/oats types",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "yarn ts-node examples/driver.ts && yarn jest && yarn render",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rm -rf ./dist && rm -f ./tmp/*.ts",
    "build": "yarn clean && yarn lint && yarn test && yarn tsc",
    "prepublish": "yarn build",
    "render": "yarn ts-node render.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats-fast-check.git"
  },
  "peerDependencies": {
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "5.0.1",
    "lodash": "4.17.19",
    "randexp": "0.5.3"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats": "2.5.2",
    "@smartlyio/oats-runtime": "2.6.3",
    "@types/jest": "26.0.5",
    "@types/lodash": "4.14.157",
    "@types/node": "13.13.14",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-prettier": "3.1.4",
    "fast-check": "1.26.0",
    "jest": "26.4.1",
    "jsverify": "0.8.4",
    "prettier": "2.0.5",
    "ts-jest": "26.2.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(examples|test)/.*\\.spec.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "testMatch": null
  }
}
