{
  "name": "@smartlyio/oats-fast-check",
  "version": "0.1.2",
  "license": "MIT",
  "description": "fast-check value generator for smartlyio/oats types",
  "private": false,
  "author": "Smartly.io",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "yarn ts-node examples/driver.ts && yarn jest && yarn render",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rm -rf ./dist && rm -f ./tmp/*.ts",
    "build": "yarn clean && yarn lint && yarn test && yarn tsc",
    "prepublish": "yarn build",
    "render": "yarn ts-node render.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smartlyio/oats-fast-check.git"
  },
  "peerDependencies": {
    "typescript": "^4.2.0"
  },
  "dependencies": {
    "@smartlyio/safe-navigation": "^5.0.1",
    "lodash": "^4.17.20",
    "randexp": "^0.5.3"
  },
  "keywords": [
    "oats",
    "openapi3",
    "rest",
    "generator",
    "typescript",
    "server",
    "client"
  ],
  "devDependencies": {
    "@smartlyio/oats": "2.16.2",
    "@smartlyio/oats-runtime": "2.13.2",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.170",
    "@types/node": "14.17.4",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-prettier": "3.4.0",
    "fast-check": "2.17.0",
    "jest": "27.0.5",
    "jsverify": "0.8.4",
    "prettier": "2.3.2",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "typescript": "4.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(examples|test)/.*\\.spec.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "preset": "ts-jest",
    "testMatch": null
  }
}
